//Find the square root of given number
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int squareroot(int n){
    int i=1;
    while(i*i<=n){
        i++;
    }
    return i-1;
}
int main(){
    int n;
    cout<<"The number is ";
    cin>>n;
    cout<<"The square root of num is "<<squareroot(n);
  
}
