#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
    int arr[]={1,4,5,2,3};
    int n=(sizeof(arr)/sizeof(arr[0]));
    sort(arr,arr+n);
    for(int i:arr){
        cout<<i<<" ";
    }
    cout<<endl;
    sort(arr,arr+n,greater<int>());
    for(int i:arr){
        cout<<i<<" ";
    }
    return 0;
}
