#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
    vector<int>v={1,4,3,8,6};
    sort(v.begin(),v.end());
    for(int i:v){
        cout<<i<<" ";
    }
    cout<<endl;
    sort(v.begin(),v.end(),greater<int>());
    for(int i:v){
        cout<<i<<" ";
    }
    return 0;
}
